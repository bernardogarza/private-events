<h1>My Events</h1>

<% @myevents.each do |e| %>
    <h2>Title:</h2>
    <h3><%= e.title %></h3>
    <h2>Location:</h2>
    <p><%= e.location %></p>
    <h2>Description:</h2>
    <p><%= e.description %></p>
    <h2>Date and time:</h2>
    <p><%= e.date %></p>
    <h2>Created at:</h2>
    <p><%= e.created_at %></p>
<% end %>