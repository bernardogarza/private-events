<ul>
    <li> <%= link_to  "All Events", events_path %> </li>
    <% if logged_in? %>
      <li> <%= link_to  "Create Event", new_event_path %> </li>
      <li> <%= link_to  "Log Out", sessions_destroy_path, method: :delete %> </li>
      <%= current_user.user_name %>
    <% else %>
      <li><%= link_to  "Log-in", sessions_new_path %></li>
      <li><%= link_to  "Sign up", users_new_path %></li>
    <% end %>
</ul>