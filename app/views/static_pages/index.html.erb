<% if flash[:success] %>
    <div class="alert alert-success"><%= flash[:success] %></div>
<% end %>
<div class="padding">
    <div class="form-container">
        <div class="users">
            <h1>Users</h1>

            <% User.all.each do |u| %>
                <li> <%= link_to  "#{u.user_name}", user_path(u.id) %> </li>
            <% end %>
        </div>
    </div>
</div>
